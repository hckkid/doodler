<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <h1 class="lead">Chat Room <small><strong><%= roomno %></strong></small></h1>
    <ul class="list-group" id="messages_container"></ul>
  </div>
</div>
<div class="row">
  <video id="webrtc-sourcevid" autoplay style="width: 320px; height: 240px; border: 1px solid black;"></video>
  <button type="button" id="startVideo">Start video</button>
  <button type="button" id="stopVideo">Stop video</button>
  <video id="webrtc-remotevid" autoplay style="width: 320px; height: 240px; border: 1px solid black;"></video>
  <button type="button" id="connect">Connect</button>
  <button type="button" id="hangUp">Hang Up</button>
</div>
<div name="localCanvas" id="localCanvas" class="row">
  <canvas id="lcanvas" name="lcanvas" height="200" width="200">
  </canvas>
  <div name="remoteCanvases" id="remoteCanvases">
    <ul class="list-inline" id="rcanvas"></ul>
  </div>
</div>
<div style="position: fixed;bottom: 0;left: 0;width: 100%;padding: 25px 0;background: #fff;">
  <div class="container">
    <div class="row">
      <form name="chatbox">
        <label id="messages"></label>
        <input type="text" placeholder="message" id="locMessage" name="message" />
        <input type="hidden" name="roomno" id="roomno" value=<%= roomno %> />
        <button type="submit">Send Message</button>
      </form>
  </div>
</div>
</div>
